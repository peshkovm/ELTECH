+<%@ page import="java.util.List" %>
+<%@ page import="java.util.Map" %>
+<%@ page import="java.net.URLDecoder" %>
+<%@ page language="java" contentType="text/html; charset=UTF-8"
+         pageEncoding="UTF-8" %>
+<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
+"http://www.w3.org/TR/html4/loose.dtd">
+<html>
+<head>
+    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
+    <title>Отфильтрованный список команд</title>
+</head>
+<body>
+<%
+    Cookie[] c = request.getCookies();
+    String color = null;
+    for (Cookie i : c) {
+        if (i.getName().equals("color"))
+            color = i.getValue();
+    }
+    for (Cookie i : c) {
+        if (i.getName().equals("name")) {
+            out.print("<p style=\"color:" + color + "\";" + " name='" + URLDecoder.decode(i.getValue(), "UTF-8") + "</p>\n");
+        }
+        if (i.getName().equals("color")) {
+            out.print("<p style=\"color:" + color + "\";" + "color='" + URLDecoder.decode(i.getValue(), "UTF-8") + "</p>\n");
+        }
+    }
+    HttpSession session1 = request.getSession();
+    out.print("<font color=\"" + color + "\">" + " count='" + URLDecoder.decode(session1.getAttribute("count").toString(), "UTF-8") + "</font><br>");
+    out.print("<font color=\"" + color + "\">" + " date='" + URLDecoder.decode(session1.getAttribute("date").toString(), "UTF-8") + "</font><br>");
+%>
+</body>
+</html>
